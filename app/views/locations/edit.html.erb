

<div class="booyah-box col-10 offset-1">
  <h1>Edit Your Location</h1>
  <%= simple_form_for @location do |f| %>
    <%= f.input :address %>
    <%= f.input :headline %>
    <%= f.input :description %>
    <%= f.input :danger_level %>
    <%= f.submit 'Edit', class: 'btn btn-primary' %>
  <% end %>
</div>